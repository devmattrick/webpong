{
  "name": "webpong",
  "version": "1.0.0",
  "description": "A multiplayer pong revamp powered by HTML5.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "gulp dev",
    "dev:server": "nodemon --watch src/server/ src/server/index.js --exec babel-node",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "cross-env NODE_ENV=production gulp build",
    "build:server": "babel src/server/ -d dist/server/",
    "start": "node dist/server/index.js"
  },
  "keywords": [
    "pong",
    "multiplayer"
  ],
  "author": "mattrick <git@mattrick.me>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.13",
    "concurrently": "^3.5.0",
    "critical": "^1.1.0",
    "cross-env": "^5.0.5",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^7.0.0",
    "gulp-cachebust": "^0.0.9",
    "gulp-cssmin": "^0.2.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.0.0",
    "gulp-purifycss": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "gulp-sass": "^3.1.0",
    "gulp-sequence": "^0.4.6",
    "gulp-uglify": "^3.0.0",
    "gulp-wait": "^0.0.2",
    "gulp-watch": "^4.3.11",
    "lazypipe": "^1.0.1",
    "nodemon": "^1.12.1",
    "require-dir": "^0.3.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-stream": "^1.24.1",
    "uglify-es": "^3.1.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "workbox-build": "^2.1.0",
    "workbox-cli": "^2.1.0"
  },
  "dependencies": {
    "express": "^4.16.2"
  }
}
